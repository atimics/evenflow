# Example .env file for Evenflow MCP server
# Copy to .env and fill in values

# Database connection (leave empty for in-memory mode)
DATABASE_URL=

# AWS Configuration (for deployment)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# MCP Server configuration
MCP_SERVER_NAME=evenflow-mcp
MCP_SERVER_PORT=8080

# Logging
LOG_LEVEL=INFO
